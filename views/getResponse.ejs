        <section class="container">
            <div class="col-md-7 contact-form">
                <form action="">
                    <div class="input-field">
                        <input type="number" name="age" class="form-control" placeholder="Your Age">
                    </div>

                    <div class="input-field">
                        <select name="gender" id="gender">
                            <option value="">Select Your Gender</option>
                            <option value="Male">Male</option>
                            <option value="Female">Female</option>
                            <option value="Other">Other</option>
                            <option value="notSay">Prefer Not To Say</option>
                        </select>
                    </div>

                    <div class="input-field">
                        <select name="relationship_status" id="relationship_status">
                            <option value="">Select Your Relationship Status</option>
                            <option value="In a Relationship">In a Relationship</option>
                            <option value="Single">Single</option>
                            <option value="Married">Married</option>
                            <option value="Divorced">Divorced</option>
                        </select>
                    </div>

                    <div class="input-field">
                        <select name="occupation" id="occupation">
                            <option value="">Select Your Occupation</option>
                            <option value="Salaried Worker">Salaried Worker</option>
                            <option value="Retired">Retired</option>
                            <option value="Student">School Student</option>
                            <option value="Divorced">University Student</option>
                            <option value="Other">Other</option>
                        </select>
                    </div>

                    <div class="slidecontainer input-field">
                        <input type="range" min="1" max="5" value="2" class="slider" id="myRange">
                    </div>

                    <div class="input-field">
                        <input type="radio" name="subject" class="form-control" placeholder="Subject...">
                    </div>
                    <div class="input-field">
                        <input type="text" name="subject" class="form-control" placeholder="Subject...">
                    </div>

                </form>
            </div>
        </section>



