<!DOCTYPE html>

<html lang="en" class="no-js">


<head>
    <!-- meta character set -->
    <meta charset="utf-8">
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>Blue Pro</title>
    <!-- Meta Description -->
    <meta name="description" content="Blue One Page Creative HTML5 Template">
    <meta name="keywords" content="one page, single page, onepage, responsive, parallax, creative, business, 
        html5, css3, css3 animation, mental, health, social media, facebook, instagram, reddit, discord, YouTube">
    <meta name="author" content="Tyler Sheffield, Allan Foote, Mikayla Randall, Rai Martin">

    <!-- Mobile Specific Meta -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- theme meta -->
    <meta name="theme-name" content="blue-pro" />

    <!-- CSS
		================================================== -->

    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,700">
    <link rel="shortcut icon" type="image/x-icon" href="/images/icon.png" />
    <!-- Fontawesome Icon font -->
    <link rel="stylesheet" href="plugins/font-awesome.min.css">
    <!-- Ion Icons Font -->
    <link rel="stylesheet" href="plugins/ionicons.min.css">
    <!-- bootstrap.min -->
    <link rel="stylesheet" href="plugins/jquery.fancybox.css">
    <!-- bootstrap.min -->
    <link rel="stylesheet" href="plugins/bootstrap/bootstrap.min.css">
    <!-- bootstrap.min -->
    <link rel="stylesheet" href="plugins/owl.carousel.css">
    <!-- bootstrap.min -->
    <link rel="stylesheet" href="plugins/slit-slider.css">
    <!-- bootstrap.min -->
    <link rel="stylesheet" href="plugins/animate.css">
    <!-- Main Stylesheet -->
    <link rel="stylesheet" href="css/style.css">
    <link rel="stylesheet" href="css/response.css">
    <link rel="stylesheet" href="css/results.css">

    <!-- Modernizer Script for old Browsers -->
    <script src="plugins/modernizr-2.6.2.min.js"></script>


</head>

<body id="body">

    <!-- preloader -->
    <!-- <div id="preloader">
				<div class="loder-box">
					<div class="battery"></div>
				</div>
		</div> -->
    <!-- end preloader -->

    <!--
        Fixed Navigation
        ==================================== -->
    <header id="navigation" class="fixed-top animated-header">
        <div class="container">
            <nav class="navbar navbar-expand-lg navbar-dark">
                <a class="navbar-brand" href="/">
                    <img class="img-fluid" src="images/logo.png" alt="">
                </a>
                <button class="navbar-toggler" type="button" data-toggle="collapse"
                    data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                    aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav ml-auto text-center" id="nav">
                        <li class="nav-item active">
                            <a class="nav-link" href="/">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/dashboard">Dashboard</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/login">Login</a>
                        </li>


                    </ul>
                </div>
            </nav>
        </div>
    </header>

    <section class="service-2 section">
        <table class="table-style">
            <thead>
                <tr>
                    <th>
                        Survey Number
                    </th>
                    <th>
                        Time Stamp
                    </th>
                    <th>
                        Age
                    </th>
                    <th>
                        Location
                    </th>
                    <th>
                        SM Usage
                    </th>
                    <th>
                        Time Spent
                    </th>
                    <th>
                        freqdistract
                    </th>
                    <th>
                        oftendistract
                    </th>
                    <th>
                        restless
                    </th>
                    <th>
                        easilydistract
                    </th>
                    <th>
                        howmuchworry
                    </th>
                    <th>
                        difficultyconcentrating
                    </th>
                    <th>
                        oftencomparison
                    </th>
                    <th>
                        comparefeel
                    </th>
                    <th>
                        seekvalid
                    </th>
                    <th>
                        feeldepressed
                    </th>
                    <th>
                        changeinterest
                    </th>
                    <th>
                        sleepissues
                    </th>
                    <th>
                        gender
                    </th>
                    <th>
                        relationship
                    </th>
                    <th>
                        occupationNum
                    </th>
                    <th>
                        occupation
                    </th>
                    <th>
                        socialNum
                    </th>
                    <th>
                        social
                    </th>

                </tr>
            </thead>
            <tbody>
                <% for (let iCount=0; iCount < surveyresults.length; iCount++) { %>
                    <tr>
                        <td>
                            <%= surveyresults[iCount].survey_id %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].time_stamp %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].age %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].location %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].use_social %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].time_spent_on_social_media %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].frequency_of_social_media_distraction %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].how_often_distracted %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].feel_restless %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].how_easily_distracted %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].how_much_worry %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].difficulty_concentrating %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].how_often_compare %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].comparison_feelings %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].seek_validation_from_social_media %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].how_often_depressed %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].frequency_of_changing_interests %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].how_often_sleep_issues %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].gender_description %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].relationship_status_description %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].occupation_description %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].organization_number %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].organization %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].social_media_number %>
                        </td>
                        <td>
                            <%= surveyresults[iCount].social_media_platform %>
                        </td>
                    </tr>
                    <% } %>
            </tbody>
        </table>
    </section>

    <!-- Twitter Bootstrap -->
    <script src="plugins/bootstrap/bootstrap.min.js"></script>
    <!-- Single Page Nav -->
    <script src="plugins/jquery.singlePageNav.min.js"></script>

    <!-- onscroll animation -->
    <script src="plugins/wow.min.js"></script>
    <!-- Custom Functions -->

    <script src="js/main.js"></script>



</body>

</html>